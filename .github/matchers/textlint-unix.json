{
  "problemMatcher": [
    {
      "owner": "textlint-unix",
      "pattern": [
        {
          "regexp": "^.+(/content/.+):(\\d+):(\\d+): ([^\\]]+?) \\[([^/]+)/([^\\]]+)\\]$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4,
          "severity": 5,
          "code": 6
        }
      ]
    }
  ]
}
